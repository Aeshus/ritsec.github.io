---
import { getCollection } from "astro:content";
import { formatDates } from "@/utils.js";
import Schedule from "@/components/Schedule.jsx";

interface Props {
    group?: string;
    count?: number;
}

const { group, count } = Astro.props;

const allEvents = await getCollection("schedule");
---

